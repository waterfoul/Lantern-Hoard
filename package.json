{
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "^1.17.1",
    "bootstrap-sass": "^3",
    "bootstrap-social": "^5.1.1",
    "bootswatch-sass": "^3.2.0",
    "chai-as-promised": "^6.0.0",
    "cookie-session": "^2.0.0-beta.1",
    "express": "^4.15.2",
    "express-winston": "^2.3.0",
    "font-awesome": "^4.7.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-github2": "^0.1.10",
    "passport-google-oauth": "^1.0.0",
    "pg": "^6.1.4",
    "pg-hstore": "^2.3.2",
    "pg-native": "^1.10.0",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sequelize": "^3.30.2",
    "sockjs": "^0.3.18",
    "sockjs-client": "^1.1.2",
    "webpack-dev-server": "^2.4.2",
    "winston": "^2.3.1",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "concurrently": "^3.4.0",
    "cross-env": "^3.2.4",
    "eslint": "^3.17.1",
    "eslint-config-fullstack": "^2.7.0",
    "eslint-plugin-react": "^6.10.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "redux-devtools-extension": "^2.13.0",
    "webpack": "^2.2.1",
    "webpack-livereload-plugin": "^0.10.0"
  },
  "scripts": {
    "build": "npm run build-js && npm run build-css",
    "build-js": "webpack",
    "build-css": "node-sass --source-map ./browser/static/index.css.map --output-style compressed browser/index.scss -o ./browser/static/",
    "build-js-watch": "npm run build-js -- -w",
    "build-css-watch": "npm run build-css && npm run build-css -- -w",
    "build-js-dev": "cross-env NODE_ENV=development npm run build-js-watch",
    "start": "node . -p $PORT",
    "start-watch": "nodemon . --watch server --watch common --",
    "start-dev": "cross-env NODE_ENV=development npm run start-watch -- --config $HOME/.lantern-hoard.json -vv",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others --prefix \"[{name}]\" --names \"   JS,SERVE,  CSS\" -c \"bgBlue.bold.blue,bgGreen.bold.green,bgBlack.bold.white\" \"npm run build-js-dev\" \"npm run start-dev\" \"npm run build-css-watch\"",
    "test": "mocha --compilers js:babel-register browser/**/*.test.js browser/**/*.test.jsx server/**/*.test.js common/**/*.test.js browser/**/**/*.test.js",
    "test-watch": "npm run test -- --watch",
    "seed": "node server/db/seed.js",
    "heroku-prebuild": "./bin/heroku-prebuild.sh",
    "lint": "eslint --ext js,jsx .",
    "ci": "npm --silent run lint && npm --silent run test",
    "fix": "eslint --ext js,jsx . --fix"
  }
}
